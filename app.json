      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "0e91c315-0f68-44cf-a5c9-ff1ae105e330"
      }
    },
    "owner": "dotcchix",
    "android": {
      "package": "com.dotcchix.taskscheduler"
    }
  }
}
